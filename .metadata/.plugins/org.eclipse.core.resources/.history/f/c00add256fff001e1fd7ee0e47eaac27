package app;

import util.Algoritmos;

public class Principal {

	public static void main(String[] args) {
		int a=3;
		Algoritmos.esPrimo(a);
		
		int [] arraynumeros=new int[5];
		for(int i=0; i<arraynumeros.length;i++) {
			arraynumeros[i]=i;
			Algoritmos.esPrimo(arraynumeros[i]);
		}
		

	}

}
